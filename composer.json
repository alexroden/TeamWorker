{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alt-three/bus": "^2.0",
        "alt-three/throttle": "^1.1",
        "alt-three/validator": "^1.5",
        "doctrine/dbal": "^2.5",
        "graham-campbell/binput": "~3.3",
        "graham-campbell/exceptions": "^9.0",
        "graham-campbell/markdown": "~7.0",
        "laravel/framework": "^5.4",
        "laravel/passport": "^1.0",
        "mccool/laravel-auto-presenter": "^5.0",
        "nikic/php-parser": "^3.0",
        "laravel/tinker": "^1.0",
        "laravel/browser-kit-testing": "^1.0"
    },
    "require-dev": {
        "alt-three/testbench": "^1.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.4",
        "graham-campbell/testbench-core": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
          "app/helpers.php"
        ],
        "psr-4": {
            "ABR\\TeamWorker\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ABR\\Tests\\TeamWorker\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
